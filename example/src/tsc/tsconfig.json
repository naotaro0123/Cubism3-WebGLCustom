{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": ["es2015", "dom"],
        "sourceMap": true,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "declaration": false,
        "experimentalDecorators": true,
        "rootDir": "../../../",
        "outDir": "../../.tscBuild"
    },
    "include": [
        "../../../src/*.ts",
        "./*.ts"
    ]
}